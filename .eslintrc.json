{
    "extends": "standard",
    "rules": {
        "semi": "off",
        "quotes": ["error", "single"],
        "no-trailing-spaces": "error",
        "no-extra-parens": "warn",
        "no-irregular-whitespace": "warn",
        "indent": ["error", 2, { "MemberExpression": 0 }],
        "space-before-function-paren": ["warn", "never"]
    }
}